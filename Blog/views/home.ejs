<%- include('partials/header') %>

    <h1>Home</h1>
    <p>
        <%= startingContent %>
    </p>

    <% posts.forEach(post=> { %>
        <div name="<%= post.title %> ">
            <h2>
                <%= post.title %>
            </h2>
            <p>
                <%= post.content.substring(0, 100); %>
                    ...
                    <a href=" /post/<%=post._id %> ">Read More</a>
            </p>
        </div>
        <% }) %>

            <%- include('partials/footer') %>